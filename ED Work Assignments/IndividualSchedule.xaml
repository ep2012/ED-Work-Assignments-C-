<Window x:Class="ED_Work_Assignments.IndividualSchedule"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="IndividualSchedule" Height="800" Width="800" FontFamily="Bookman Old Style" FontSize="14" Background="#FF9B9999" Left="0" Top="0">
    <Grid>
        <Label x:Name="lblSchedule" Content="*'s Schedule" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" FontSize="20" Width="764"/>
        <DatePicker x:Name="dtStart" HorizontalAlignment="Left" Height="33" Margin="194,43,0,0" VerticalAlignment="Top" Width="195" FontSize="20" CalendarClosed="CalendarClosed"/>
        <DatePicker x:Name="dtEnd" HorizontalAlignment="Left" Height="33" Margin="578,43,0,0" VerticalAlignment="Top" Width="196" FontSize="20" CalendarClosed="CalendarClosed"/>
        <Label Content="Start" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" FontSize="20" Width="179"/>
        <Label Content="End" HorizontalAlignment="Left" Margin="395,43,0,0" VerticalAlignment="Top" FontSize="20" Width="178"/>
        <DataGrid x:Name="dgSchedule" HorizontalAlignment="Left" Height="674" Margin="10,81,0,0" VerticalAlignment="Top" Width="764" FontSize="18"/>

    </Grid>
</Window>
